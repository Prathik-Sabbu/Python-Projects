if __name__ == '__main__':
    n = int(input())
    student_marks = {}
    for _ in range(n):
        name, *line = input().split()
        scores = list(map(float, line))
        student_marks[name] = scores
    query_name = input()
d = float(0)
num = student_marks[query_name]
for y in num:
   d += y
x=(d/len(num))
print(format(x, '.2f'))
