password = "Password"
user = "Prathik"
def login():
    username = input("What is your username: ")
    if username.upper() == user.upper():
        passw = input("What is your password: ")
        if passw == password:
          print('welcome back') 
        else:
          print('wronge password')
          login()
    else:
        print("Wrong username")
        login()
login()
